<dict>
	<key>beforeRunningCommand</key>
	<string>nop</string>
	<key>command</key>
	<string>#!/usr/bin/osascript
#playpause
tell application "iTunes"
  playpause
  if not(exists current track) then "unknown"
  (get name of current track) &amp; "\nFrom " &amp; (get album of current track) &amp; "\nBy " &amp; (get artist of current track) &amp; "\nin " &amp; (get year of current track)
end tell</string>
	<key>input</key>
	<string>none</string>
	<key>keyEquivalent</key>
	<string>$@ÔúÅ</string>
	<key>name</key>
	<string>Play/Pause</string>
	<key>output</key>
	<string>showAsTooltip</string>
